@thesis{ota:2023,
  author       = {Iara Naomi Nobre Ota},
  title        = {Optimizing the LIGO Summary Pages Pipeline},
  year         = {2023},
  month        = {dec},
  date         = {2023-12-20},
  institution  = {Unknown Institution}, % Replace this with the correct institution if available
  abstract     = {The LIGO summary pages are web-based graphical summaries of the performance of the LIGO gravitational wave detectors and their many subsystems. These pages are essential for the ongoing monitoring and noise characterization efforts. Therefore, it is crucial to optimize the pipeline that generates these pages. In this work, we present a new workflow for the LIGO Summary Pages pipeline, which takes advantage of the High-Throughput Computing (HTC) paradigm to parallelize the processing and creation of the webpages. The new workflow was implemented by subdividing the process into smaller components that can be executed independently and concurrently on small compute nodes. With this new workflow, each process runtime was reduced to a median runtime of 10 ±5 minutes.},
  url          = {https://hdl.handle.net/20.500.11767/139990},
  note         = {Accessed: 2024-09-02},
}
























































